<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Kathmandu"
    android:background="#030303"
    android:orientation="vertical"
    >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tourist Attraction In Kathmandu"
            android:textSize="22dp"
            android:fontFamily="sans-serif-condensed"
            android:textAlignment="center"
            android:textColor="#ffffff"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1"
            >
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/pashupatinath"
                android:layout_weight="1"
                android:onClick="pasunath"
                android:scaleType="fitXY"
                />
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/swayambhunath"
                android:layout_weight="1"
                android:onClick="swayambhunath"
                android:scaleType="fitXY"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pashupatinath"
                android:layout_weight="1"
                android:textAlignment="center"
                android:textColor="#ffffff"

                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Swayambhunath"
                android:layout_weight="1"
                android:textAlignment="center"
                android:textColor="#ffffff"
                />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1"

            >

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/budhanilkantha"
                android:layout_weight="1"
                android:onClick="Budhanilkanth"
                android:scaleType="fitXY"
                />
            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/bhaktapurdurbarsquare"
                android:layout_weight="1"
                android:onClick="Bhaktapur_Darbar_Square"
                android:scaleType="fitXY"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Budhanilkanth"
                android:layout_weight="1"
                android:textAlignment="center"
                android:textColor="#ffffff"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bhaktapur Darbar Square"
                android:layout_weight="1"
                android:textAlignment="center"
                android:textColor="#ffffff"
                />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1"

            >
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/sanga"
                android:layout_weight="1"
                android:onClick="Sanga"
                android:scaleType="fitXY"
                />
            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/patan"
                android:layout_weight="1"
                android:onClick="Patan_Darbar_Square"
                android:scaleType="fitXY"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sanga"
                android:layout_weight="1"
                android:textAlignment="center"
                android:textColor="#ffffff"

                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Patan Darbar Square"
                android:layout_weight="1"
                android:textAlignment="center"
                android:textColor="#ffffff"
                />
        </LinearLayout>
    </LinearLayout>


